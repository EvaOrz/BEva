<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white_bg" >

    <RelativeLayout
        android:id="@+id/my_coin_notice_bar_layout"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/bar_height"
        android:background="@color/title_bar" >

        <ImageView
            android:id="@+id/my_coin_notice_button_back"
            android:layout_width="@dimen/bar_height"
            android:layout_height="@dimen/bar_height"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/nav_button_margin_left"
            android:contentDescription="@null"
            android:src="@drawable/ic_back" />

        <TextView
            android:id="@+id/my_coin_notice_bar_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/shop_use_notice"
            android:textColor="@color/black_bg"
            android:textSize="@dimen/dp18_textsize" />

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:contentDescription="@null"
            android:scaleType="fitXY"
            android:src="@drawable/list_item_divider" />
    </RelativeLayout>

    <TextView
        android:id="@+id/my_coin_notice_user_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/my_coin_notice_bar_layout"
        android:layout_marginLeft="@dimen/my_coin_notice_user_name_margin_left"
        android:layout_marginTop="@dimen/my_coin_notice_user_name_margin_top"
        android:text="@string/welcom_user"
        android:textColor="@color/black_bg"
        android:textSize="@dimen/dp18_textsize" />

    <TextView
        android:id="@+id/my_coin_notice_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/my_coin_notice_user_name"
        android:layout_below="@id/my_coin_notice_user_name"
        android:layout_marginRight="@dimen/my_coin_notice_user_name_margin_left"
        android:layout_marginTop="@dimen/my_coin_notice_content_margin_top"
        android:text="@string/notice_content"
        android:textColor="@color/black_bg"
        android:textSize="@dimen/dp14_textsize" />

    <ImageView
        android:id="@+id/my_coin_notice_ufo"
        android:layout_width="@dimen/my_coin_ufo_width"
        android:layout_height="@dimen/my_coin_ufo_height"
        android:layout_below="@id/my_coin_notice_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/my_coin_notice_ok_margin_top"
        android:contentDescription="@null"
        android:src="@drawable/my_coin_ufo"
        android:visibility="invisible" />

    <Button
        android:id="@+id/my_coin_notice_ok"
        android:layout_width="@dimen/my_coin_notice_ok_width"
        android:layout_height="@dimen/my_coin_notice_ok_height"
        android:layout_below="@+id/my_coin_notice_ufo"
        android:layout_centerHorizontal="true"
        android:background="@drawable/login_bg"
        android:text="@string/my_coin_ok"
        android:textColor="@color/white_bg" />

    <ImageView
        android:id="@+id/my_coin_notice_ship"
        android:layout_width="@dimen/my_coin_ufo_width"
        android:layout_height="@dimen/my_coin_ufo_height"
        android:layout_above="@+id/my_coin_notice_ok"
        android:layout_centerHorizontal="true"
        android:contentDescription="@null"
        android:scaleType="fitStart"
        android:src="@drawable/my_coin_ship" />

    <ImageView
        android:id="@+id/my_coin_notice_light"
        android:layout_width="@dimen/my_coin_light_width"
        android:layout_height="@dimen/my_coin_light_height"
        android:layout_above="@+id/my_coin_notice_ok"
        android:layout_centerHorizontal="true"
        android:contentDescription="@null"
        android:src="@drawable/my_coin_light" />

