<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <RelativeLayout
        android:id="@+id/nav_bar"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/nav_var_height"
        android:background="@color/column_head" >


        <TextView
            android:id="@+id/gallery_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/nav_bar_text_margin_top"
            android:shadowColor="@android:color/black"
            android:shadowDx="0.0"
            android:shadowDy="-0.5"
            android:shadowRadius="1.0"
            android:text="@string/settings_feedback"
            android:textColor="@android:color/white"
            android:textSize="@dimen/dp11_textsize" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/nav_bar"
        android:background="@color/black_bg" >

        <WebView
            android:id="@+id/feedback_web_view"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" />
    </RelativeLayout>

    <View
        android:id="@+id/top_shadow"
        android:layout_width="fill_parent"
        android:layout_height="3dp"
        android:layout_below="@id/nav_bar"
        android:background="@drawable/nav_bar_shadow" />

