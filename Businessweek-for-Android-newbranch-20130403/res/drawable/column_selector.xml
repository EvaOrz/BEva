<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 没选中的图片 -->
    <item android:drawable="@drawable/left_cell" android:state_window_focused="false"/>
    <!-- 选中的图片 -->
    <item android:drawable="@drawable/left_cell_tapped" android:state_focused="true" android:state_pressed="true"/>
    <item android:drawable="@drawable/left_cell_tapped" android:state_focused="false" android:state_pressed="true"/>
    <item android:drawable="@drawable/left_cell_tapped" android:state_selected="true"/>
    <item android:drawable="@drawable/left_cell_tapped" android:state_focused="true"/>

</selector>